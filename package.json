{
  "name": "tree-sitter-voloscript",
  "version": "0.1.0",
  "scripts": {
    "install": "tree-sitter generate && tree-sitter build"
  },
  "dependencies": {
    "tree-sitter-cli": "^0.26.3"
  },
  "tree-sitter": [
    {
      "scope": "source.voloscript",
      "file-types": [
        "script"
      ],
      "injection-regex": "^voloscript$",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
